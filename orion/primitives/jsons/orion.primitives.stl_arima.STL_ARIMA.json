{
    "name": "orion.primitives.stl_arima.STL_ARIMA",
    "contributors": [
        "Ajay Arora <ajarora@mit.edu>"
    ],
    "description": "STL-decomposed ARIMA Model",
    "classifiers": {
        "type": "estimator",
        "subtype": "regressor"
    },
    "modalities": [],
    "primitive": "orion.primitives.stl_arima.STL_ARIMA",
    "produce": {
        "method": "predict",
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "y",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "steps": {
                "type": "int",
                "default": 10
            }
        },
        "tunable": {
            "p": {
                "type": "int",
                "default": 1,
                "range": [
                    0,
                    40
                ]
            },
            "d": {
                "type": "int",
                "default": 0,
                "range": [
                    0,
                    3
                ]
            },
            "q": {
                "type": "int",
                "default": 0,
                "range": [
                    0,
                    12
                ]
            }
        }
    }
}
